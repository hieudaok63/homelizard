{
  "name": "@homelizard/expo",
  "version": "0.1.0",
  "main": "index.tsx",
  "scripts": {
    "clean": "git clean -xdf .expo .turbo node_modules",
    "dev": "pnpm generate:locales && expo start --ios",
    "dev:android": "pnpm generate:locales && expo start --android",
    "dev:ios": "pnpm generate:locales && expo start --ios",
    "dev:client": "pnpm generate:locales && expo start --dev-client",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "type-check": "tsc --noEmit",
    "build:local": "pnpm generate:locales && eas build --platform ios -e dev --local",
    "generate:locales": "node ./generateLocales.mjs",
    "watch:locales": "pnpm generate:locales && cd ../../ && watchman-make -p 'apps/nextjs/public/locales/**/*.json' --run 'cd apps/expo && pnpm generate:locales'"
  },
  "dependencies": {
