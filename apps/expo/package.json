{
  "name": "@homelizard/expo",
  "version": "0.1.0",
  "main": "index.tsx",
  "scripts": {
    "eas-build-post-install": "pnpm generate:locales",
    "clean": "git clean -xdf .expo .turbo node_modules",
    "dev": "pnpm generate:locales && pnpm with-env expo start --ios",
    "dev:android": "pnpm generate:locales && expo start --android",
    "dev:android-window": "expo start --android",
    "dev:ios": "pnpm generate:locales && expo start --ios",
    "dev:client": "pnpm generate:locales && expo start --dev-client",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "type-check": "tsc --noEmit",
    "build:local": "pnpm generate:locales && eas build --platform ios -e dev --local",
    "generate:locales": "node ./generateLocales.mjs",
    "watch:locales": "pnpm generate:locales && cd ../../ && watchman-make -p 'apps/nextjs/public/locales/**/*.json' --run 'cd apps/expo && pnpm generate:locales'",
    "with-env": "dotenv -e ../../.env --",
    "build:ios": "EAS_LOCAL_BUILD_ARTIFACTS_DIR=.build eas build --platform ios -e preview --local --non-interactive",
    "build:android": "EAS_LOCAL_BUILD_ARTIFACTS_DIR=.build eas build --platform android -e preview --local --non-interactive"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.335.0",
    "@aws-sdk/s3-request-presigner": "^3.335.0",
    "@clerk/clerk-expo": "^0.16.3",
    "@expo-google-fonts/nunito": "^0.2.3",
    "@expo/metro-config": "^0.7.1",
    "@homelizard/api": "*",
    "@homelizard/schema": "*",
    "@homelizard/tailwind-config": "*",
    "@hookform/resolvers": "^3.1.0",
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-community/datetimepicker": "6.7.3",
    "@react-native-masked-view/masked-view": "0.2.9",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/elements": "^1.3.17",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@shopify/flash-list": "1.4.3",
    "@tanstack/react-query": "^4.29.23",
    "@trpc/client": "^10.35.0",
    "@trpc/react-query": "^10.35.0",
    "dayjs": "^1.11.7",
    "expo": "^48.0.19",
    "expo-asset": "^8.9.1",
    "expo-auth-session": "^4.0.3",
    "expo-build-properties": "^0.6.0",
    "expo-constants": "~14.2.1",
    "expo-font": "~11.1.1",
    "expo-image": "~1.0.1",
